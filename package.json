    "watch:webpack": "webpack --watch"
  },
  "dependencies": {
    "@jupyterlab/application": "^0.19.1",
    "@jupyterlab/apputils": "^0.19.1",
    "font-awesome": "~4.7.0"
  },
  "devDependencies": {
    "style-loader": "~0.22.1",
    "css-loader": "~0.28.7",
    "extract-text-webpack-plugin": "~2.1.2",
    "npm-run-all": "~4.1.1",
    "rimraf": "~2.6.2",
    "svg-url-loader": "~2.3.1",
    "svgo": "~1.0.4",
    "svgo-loader": "~2.1.0",
    "typescript": "~2.8.3",
    "url-loader": "~0.5.9",
    "webpack": "~2.7.0"
